    <script>
        function downlaodpdf() {
            var element = document.getElementById('invoice');

            const h2 = document.querySelectorAll('h2');
            const h4 = document.querySelectorAll('h4');

            for (let i = 0; i < h2.length; i++) {
                h2[i].style.fontSize = '30px';
