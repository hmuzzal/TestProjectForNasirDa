<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <!-- Tell the browser to be responsive to screen width -->
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="">
    <!-- Favicon icon -->
    <link rel="icon" type="image/png" sizes="16x16" href="~/Resources/Organization/icon.ico">
    <title>AL AREEQ Ltd.</title>
    <!-- chartist CSS -->
    <link href="~/Content/assets/nodepackages/morrisjs/morris.css" rel="stylesheet">
    <link href="~/Content/dist/css/pages/ecommerce.css" rel="stylesheet">
    <!-- Custom CSS -->
    <link href="~/Content/dist/css/style.min.css" rel="stylesheet">
    <!-- page css -->
    <link href="~/Content/dist/css/pages/float-chart.css" rel="stylesheet">

    <style>
        .inputField {
            display: block;
            width: 100%;
            /*  background: #E8E8EC;  */
            /*background: #F5F3F3;*/
            font-family: Montserrat-Bold;
            font-size: 16px;
            line-height: 1.5;
            color: black;
            font-weight: 500;
            outline: none;
            /* border: 1px solid #03A9F3;
            border: 1px solid #F0ECEC; */
            box-shadow: 0 2px 2px rgba(0,0,0, 0.4);
            /*border: 1px solid #F0ECEC;*/
        }

            .inputField:focus {
                box-shadow: 0 0 5px #4F5467;
                /*border: 2px solid #4F5467;*/
            }

        .input-lable {
            color: #4F5467;
            font-size: 16px !important;
            font-weight: 400;
            margin-bottom: 5px;
            display: inline-block;
            font-family: montserrat-bold;
        }

        .require-symbol {
            color: red;
        }
    </style>

</head>

<body class="skin-default fixed-layout">
    <!-- ============================================================== -->
    <!-- Preloader - style you can find in spinners.css -->
    <!-- ============================================================== -->
    <div class="preloader">
        <div class="loader">
            <div class="loader__figure"></div>
            <p class="loader__label">AL AREEQ Ltd.</p>
        </div>
    </div>
    <!-- ============================================================== -->
    <!-- Main wrapper - style you can find in pages.scss -->
    <!-- ============================================================== -->
    <div id="main-wrapper">
        <!-- ============================================================== -->
        <!-- Topbar header - style you can find in pages.scss -->
        <!-- ============================================================== -->
        <header class="topbar">
            <nav class="navbar top-navbar navbar-expand-md navbar-dark">
                <!-- ============================================================== -->
                <!-- Logo -->
                <!-- ============================================================== -->
                <div class="navbar-header">
                    <a class="navbar-brand" href="@Url.Action("Index", "Home")">
                        <!--You can put here icon as well // <i class="wi wi-sunset"></i> //-->
                        <!-- Dark Logo icon -->
                        <img src="~/Resources/Organization/150X162.png" alt="homepage" class="dark-logo" />
                        <!-- Light Logo icon -->
                        <img src="~/Resources/Organization/150X162.png" alt="homepage" class="light-logo" />
                    </a>
                </div>
                <!-- ============================================================== -->
                <!-- End Logo -->
                <!-- ============================================================== -->
               
            </nav>
        </header>
        <!-- ============================================================== -->
        <!-- End Topbar header -->
        <!-- ============================================================== -->
        <!-- ============================================================== -->
        <!-- Left Sidebar - style you can find in sidebar.scss  -->
        <!-- ============================================================== -->
        @{
            int UserID = InvoiceSoftware.DataLayer.InvoiceSoftwareDBAccess.GetUserIDbyEmails(Session["Email"].ToString());
            var user = InvoiceSoftware.DataLayer.InvoiceSoftwareDBAccess.GetUserById(UserID);
        }
        <aside class="left-sidebar">
            <!-- Sidebar scroll-->
            <div class="scroll-sidebar">
                <!-- User Profile-->
                <div class="user-profile">
                    <div class="user-pro-body">
                        <div><img src="@user.ProfileImage" alt="user-img" class="img-circle"></div>
                        <div class="dropdown">
                            <a href="javascript:void(0)" class="dropdown-toggle u-dropdown link hide-menu" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">@user.FullName <span class="caret"></span></a>
                            <div class="dropdown-menu animated flipInY">
                                <!-- text-->
                                <a href="javascript:void(0)" class="dropdown-item"><i class="ti-user"></i> My Profile</a>
                                <!-- text-->
                                <div class="dropdown-divider"></div>
                                <!-- text-->
                                <a href="@Url.Action("Logout", "User")" class="dropdown-item"><i class="fa fa-power-off"></i> Logout</a>
                                <!-- text-->
                            </div>
                        </div>
                    </div>
                </div>
                <!-- Sidebar navigation-->
                <nav class="sidebar-nav">
                    <ul id="sidebarnav">

                        <li>
                            <a class="has-arrow waves-effect waves-dark" href="javascript:void(0)" aria-expanded="false"><i class="ti-layout-grid2"></i><span class="hide-menu">Profile</span></a>
                            <ul aria-expanded="false" class="collapse">
                                <li><a href="app-calendar.html">Personal Informations</a></li>
                                <li><a href="app-chat.html">Email Address</a></li>
                                <li><a href="app-chat.html">Mobile Numbers</a></li>

                            </ul>
                        </li>
                        <li>
                            <a class="has-arrow waves-effect waves-dark" href="javascript:void(0)" aria-expanded="false"><i class="ti-layout-grid2"></i><span class="hide-menu">Security</span></a>
                            <ul aria-expanded="false" class="collapse">
                                <li><a href="app-calendar.html">Password</a></li>
                                <li><a href="app-calendar.html">Allowed IP Address</a></li>
                                <li><a href="app-calendar.html">Device Sign-Ins</a></li>

                            </ul>
                        </li>
                    </ul>
                </nav>
                <!-- End Sidebar navigation -->
            </div>
            <!-- End Sidebar scroll-->
        </aside>
        <!-- ============================================================== -->
        <!-- End Left Sidebar - style you can find in sidebar.scss  -->
        <!-- ============================================================== -->
        <!-- ============================================================== -->
        <!-- Page wrapper  -->
        <!-- ============================================================== -->
        <div class="page-wrapper">
            <!-- ============================================================== -->
            <!-- Container fluid  -->
            <!-- ============================================================== -->
            <div class="container-fluid">
                @RenderBody()


            </div>
            <!-- ============================================================== -->
            <!-- End Container fluid  -->
            <!-- ============================================================== -->
        </div>
        <!-- ============================================================== -->
        <!-- End Page wrapper  -->
        <!-- ============================================================== -->
        <!-- ============================================================== -->
        <!-- footer -->
        <!-- ============================================================== -->
        <footer class="footer">
            © 2023 JJWasteMgt
        </footer>
        <!-- ============================================================== -->
        <!-- End footer -->
        <!-- ============================================================== -->
    </div>
    <!-- ============================================================== -->
    <!-- End Wrapper -->
    <!-- ============================================================== -->
    <!-- ============================================================== -->
    <!-- All Jquery -->
    <!-- ============================================================== -->
    <script src="~/Content/assets/nodepackages/jquery/jquery-3.2.1.min.js"></script>
    <!-- Bootstrap tether Core JavaScript -->
    <script src="~/Content/assets/nodepackages/popper/popper.min.js"></script>
    <script src="~/Content/assets/nodepackages/bootstrap/dist/js/bootstrap.min.js"></script>
    <!-- slimscrollbar scrollbar JavaScript -->
    <script src="~/Content/dist/js/perfect-scrollbar.jquery.min.js"></script>
    <!--Wave Effects -->
    <script src="~/Content/dist/js/waves.js"></script>
    <!--Menu sidebar -->
    <script src="~/Content/dist/js/sidebarmenu.js"></script>
    <!--stickey kit -->
    <script src="~/Content/assets/nodepackages/sticky-kit-master/dist/sticky-kit.min.js"></script>
    <script src="~/Content/assets/nodepackages/sparkline/jquery.sparkline.min.js"></script>
    <!--Custom JavaScript -->
    <script src="~/Content/dist/js/custom.min.js"></script>
    <script src="~/Content/assets/nodepackages/jquery-sparkline/jquery.sparkline.min.js"></script>
    <!-- ============================================================== -->
    <!-- This page plugins -->
    <!-- ============================================================== -->
    <!--morris JavaScript -->
    <script src="~/Content/assets/nodepackages/raphael/raphael-min.js"></script>
    <script src="~/Content/assets/nodepackages/morrisjs/morris.min.js"></script>
    <!--Custom JavaScript -->
    <script src="~/Content/dist/js/ecom-dashboard.js"></script>

    <!-- ============================================================== -->
    <!-- This page plugins -->
    <!-- ============================================================== -->
    <!-- Chart JS -->
    <script src="~/Content/assets/nodepackages/echarts/echarts-all.js"></script>
    <script src="~/Content/assets/nodepackages/echarts/echarts-init.js"></script>

